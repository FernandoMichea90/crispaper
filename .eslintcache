arget.name]:e.target.value})\n    setetiqueta(filterItems(todasEtiquetas,e.target.value))\n}\n\n\nconst switchColor=(contador,doc)=>{\n    let objeto=[]\n    switch (contador) {\n        case 1:    \n         objeto={...doc,color:'botonMandarina'}\n         return [contador,objeto]\n          break;\n\n        case 2:    \n         objeto={...doc,color:'botonVerde'}\n        return [contador,objeto]\n        break;\n\n        case 3:    \n         objeto={...doc,color:'botonMorado'}\n        return [contador,objeto]\n        break;\n\n        case 4:    \n         objeto={...doc,color:'botonAzul'}\n        return [0,objeto]\n        break;\n    \n      \n\n    }\n\n}\n\n\n    useEffect(() => {\n             \n         pedirEtiqueta()\n\n\n\n\n    },[])\n\n    \n    return (\n        <div>\n            <div>\n            <Grid container> \n            <Grid xs={1}></Grid>\n            <Grid xs={10}>   \n            <Titulo></Titulo>\n            <Buscador buscador={buscador} filtrado={filtrado} />\n            </Grid>\n            <Grid xs={1}></Grid>\n            </Grid>\n            </div>\n\n                {cargando? \n                \n                <Progreso></Progreso>\n                :\n                \n                    <div className={clases.divMarginPrincipal} >                        \n                    \n                    <Grid container \n                    direction=\"row\"\n                    justifyContent=\"center\"\n                    alignItems=\"center\"\n                                        >\n                        {\n                         etiqueta.map(doc=> (\n                           <Grid xs={6} sm={6} md={4} lg={3} >\n\n                      \n                                <div  className={clases.divMargin}>\n                                  <Typography variant=\"h6\" align=\"center\">\n                                {doc.imagen?\n                                  <a className={clasesDos.linkImagen} onClick={()=>re